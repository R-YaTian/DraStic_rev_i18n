// Quilez shader
=============================================
<options>
name=Quilez
textures=1
</options>

<fheader>
#if GL_ES
#ifdef GL_FRAGMENT_PRECISION_HIGH
precision highp float;
#else
precision mediump float;
#endif
#endif
uniform vec4 u_texture_size;
</fheader>

<texture:0>
input=framebuffer
min_filter=GL_LINEAR
mag_filter=GL_LINEAR
</texture>

<pass>
shader=quilez.dsd
sampler:u_texture=0
</pass>
