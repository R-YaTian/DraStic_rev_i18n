// ScanlinesdColor shader
=============================================
<options>
name=ScanlinesdColor
textures=1
</options>

<fheader>
#if GL_ES
#ifdef GL_FRAGMENT_PRECISION_HIGH
precision highp float;
#else
precision mediump float;
#endif
#endif
</fheader>

<texture:0>
input=framebuffer
min_filter=GL_NEAREST
mag_filter=GL_NEAREST
</texture>

<pass>
shader=ScanlinesdColor.dsd
sampler:sampler0=0
</pass>
