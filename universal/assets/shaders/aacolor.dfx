// HQ2X shader
=============================================
<options>
name=aacolor
textures=4
</options>

<fheader>
#if GL_ES
#ifdef GL_FRAGMENT_PRECISION_HIGH
precision highp float;
#else
precision mediump float;
#endif
#endif
</fheader>

<texture:0>
input=framebuffer
min_filter=GL_NEAREST
mag_filter=GL_NEAREST
</texture>

<pass>
shader=aacolor.dsd
sampler:u_texture=0
output=1:2
</pass>
